        function updateNavigateButtonsCollectionReportExpenses() {
            if (expensesCollectionView.pageSize <= 0) {
                document.getElementById('naviagtionPageGridExpenses').style.display = 'none';
                return;
            }
            document.getElementById('naviagtionPageGridExpenses').style.display = 'block';
            btnCurrentPageGridCollectionReportExpenses.innerHTML = (expensesCollectionView.pageIndex + 1) + ' / ' + expensesCollectionView.pageCount;
        }

        function refreshExpensesFlexGrid() {
            expensesCollectionView = new wijmo.collections.CollectionView(getExpensesList());
            expensesCollectionView.canFilter = true;
            expensesCollectionView.pageSize = 15;

            var filterText = '';
            $('#expensesReportSearchInput').keyup(function () {
                filterText = this.value.toLowerCase();
                expensesCollectionView.refresh();
            });

            expensesCollectionView.filter = function (item) {
                return !filterText || (item.Area.toLowerCase().indexOf(filterText) > -1);
            }

            expensesCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsCollectionReportExpenses();
            });

            // Flex Grid
            expensesListFlexGrid.itemsSource = expensesCollectionView;
            expensesListFlexGrid.trackChanges = true;
        }

