        function getDailyCollectionRemittance() {
            var dailyCollectionRemittanceObservableArray = new wijmo.collections.ObservableArray;
            var startDate = [cboStartDateDailyCollectionAndRemittanceReport.value.getMonth() + 1, cboStartDateDailyCollectionAndRemittanceReport.value.getDate(), cboStartDateDailyCollectionAndRemittanceReport.value.getFullYear()].join('-');
            var endDate = [cboEndDateDailyCollectionAndRemittanceReport.value.getMonth() + 1, cboEndDateDailyCollectionAndRemittanceReport.value.getDate(), cboEndDateDailyCollectionAndRemittanceReport.value.getFullYear()].join('-');

            $.ajax({
                url: '/api/collections/list/dailyCollectionRemittance/' + startDate + '/' + endDate,
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (dailyCollectionRemittanceResult) {
                    if (dailyCollectionRemittanceResult.length > 0) {
                        var remarks = " ";
                        for (i = 0; i < dailyCollectionRemittanceResult.length; i++) {
                            //var newGrossCollectionDate = new Date(dailyCollectionRemittanceResult[i]["GrossCollectionDate"]);
                            //var grossCollectionDate = ('0' + (newGrossCollectionDate.getMonth() + 1)).slice(-2) + '-' + ('0' + newGrossCollectionDate.getDate()).slice(-2) + '-' + newGrossCollectionDate.getFullYear();

                            //var newNetRemittedDate = new Date(dailyCollectionRemittanceResult[i]["NetRemittedDate"]);
                            //var netRemittedDate = ('0' + (newNetRemittedDate.getMonth() + 1)).slice(-2) + '-' + ('0' + newNetRemittedDate.getDate()).slice(-2) + '-' + newNetRemittedDate.getFullYear();

                            if (parseFloat(dailyCollectionRemittanceResult[i]["NetRemitted"]) > parseFloat(dailyCollectionRemittanceResult[i]["GrossCollection"])) {
                                var differenceCollectionRemittance = parseFloat(dailyCollectionRemittanceResult[i]["NetRemitted"]) - parseFloat(dailyCollectionRemittanceResult[i]["GrossCollection"]);
                                remarks = "Over " + differenceCollectionRemittance.toLocaleString();
                            } else {
                                if (parseFloat(dailyCollectionRemittanceResult[i]["NetRemitted"]) < parseFloat(dailyCollectionRemittanceResult[i]["GrossCollection"])) {
                                    var differenceCollectionRemittance = parseFloat(dailyCollectionRemittanceResult[i]["GrossCollection"]) - parseFloat(dailyCollectionRemittanceResult[i]["NetRemitted"]);
                                    remarks = "Less " + differenceCollectionRemittance.toLocaleString();
                                } else {
                                    remarks = " ";
                                }
                            }

                            dailyCollectionRemittanceObservableArray.push({
                                Area: dailyCollectionRemittanceResult[i]["Area"],
                                //GrossCollectionDate: grossCollectionDate,
                                GrossCollection: dailyCollectionRemittanceResult[i]["GrossCollection"],
                                //NetRemittedDate: netRemittedDate,
                                NetRemitted: dailyCollectionRemittanceResult[i]["NetRemitted"],
                                Remarks: remarks
                            });
                        }
                    }

                    NProgress.done();
                }
            });

            return dailyCollectionRemittanceObservableArray;
        }

