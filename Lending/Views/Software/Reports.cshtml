                                LoanId: collectionListResult[i]["LoanId"],
                                LoanNumberDetail: collectionListResult[i]["LoanNumberDetail"],
                                LoanId: collectionListResult[i]["LoanId"],
                                LoanNumber: collectionListResult[i]["LoanNumber"],
                                StatusId: collectionListResult[i]["StatusId"],
