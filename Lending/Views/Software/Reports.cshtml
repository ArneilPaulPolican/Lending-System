        // get colllection list by collection date and area id
        function getCollectionByCollectionDateList(collectionLoanType) {
            var collectionListArray = new wijmo.collections.ObservableArray;

            var areaId = cboCollectionReportActiveArea.selectedItem["Id"];
            var startDate = [cboStartDateCollectionReportActiveReport.value.getMonth() + 1, cboStartDateCollectionReportActiveReport.value.getDate(), cboStartDateCollectionReportActiveReport.value.getFullYear()].join('-');
            var endDate = [cboEndDateCollectionReportActiveReport.value.getMonth() + 1, cboEndDateCollectionReportActiveReport.value.getDate(), cboEndDateCollectionReportActiveReport.value.getFullYear()].join('-');
            var apiUrl = '/api/collections/list/ByCollectionDate/active/' + areaId + '/' + startDate + '/' + endDate;

            if (collectionLoanType == "overdue") {
                startDate = [cboStartDateCollectionReportOverdueReport.value.getMonth() + 1, cboStartDateCollectionReportOverdueReport.value.getDate(), cboStartDateCollectionReportOverdueReport.value.getFullYear()].join('-');
                endDate = [cboEndDateCollectionReportOverdueReport.value.getMonth() + 1, cboEndDateCollectionReportOverdueReport.value.getDate(), cboEndDateCollectionReportOverdueReport.value.getFullYear()].join('-');
                areaId = cboCollectionReportOverdueArea.selectedItem["Id"];
                apiUrl = '/api/collections/list/ByCollectionDate/overdue/' + areaId + '/' + startDate + '/' + endDate;
            } else {
                if (collectionLoanType == " ") {
                    startDate = [cboStartDateCollectionReportActiveReport.value.getMonth() + 1, cboStartDateCollectionReportActiveReport.value.getDate(), cboStartDateCollectionReportActiveReport.value.getFullYear()].join('-');
                    endDate = [cboEndDateCollectionReportActiveReport.value.getMonth() + 1, cboEndDateCollectionReportActiveReport.value.getDate(), cboEndDateCollectionReportActiveReport.value.getFullYear()].join('-');
                    areaId = cboCollectionReportActiveArea.selectedItem["Id"];
                    apiUrl = '/api/collections/list/ByCollectionDate/active/' + areaId + '/' + startDate + '/' + endDate;
                }
            }

            $.ajax({
                url: apiUrl,
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
